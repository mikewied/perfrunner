[test_case]
test = perfrunner.tests.n1ql.N1QLLatencyTest
title = N1QL Q4 1Bux20Mx1K, Full Scan, 38.4KGops, 9.6KSops, 4.8KQops, GSI, stale=false
summary = Full Scan, 38.4KGops, 9.6KSops, 4.8KQops
larger_is_better = false

[cluster]
mem_quota = 20480
index_mem_quota = 100000
initial_nodes = 6
num_buckets = 1

[bucket]
replica_number = 1
password =
eviction_policy = valueOnly

[load]
items = 20000000
size = 1024
workers = 20
doc_gen = new

[n1ql]
indexes =
    #primary::CREATE PRIMARY INDEX ON `{bucket}` using gsi;

[access]
creates = 0
reads = 80
updates = 20
deletes = 0
throughput = 48000
items = 20000000
size = 1024
workers = 24
time = 1200
doc_gen = new
n1ql_queries = full-scan
n1ql_throughput = 4800
n1ql_workers = 720

[n1ql-full-scan]
statement = SELECT count(*) FROM `{bucket}` WHERE category > {category};
max-parallelism = 1
scan_consistency = request_plus
